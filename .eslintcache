ffect(() => {\n    console.log(location.pathname);\n    setIsActive(window.location.pathname);\n  }, [location.pathname]);\n\n  useEffect(() => {\n    if (openSearch) {\n      inputRef.current?.focus();\n    }\n  }, [openSearch]);\n\n  const submitSearch = (e) => {\n    e.preventDefault();\n    history.push(`/category/?q=${searchVal}`);\n  };\n\n  return (\n    <>\n      <header className=\"Header\">\n        <div className=\"HeaderBottom\">\n          <div className=\"HeaderMenu\">\n            <img\n              src={Menu}\n              onClick={openSideNav}\n              alt=\"menu\"\n              className=\"MenuBtn\"\n            />\n          </div>\n          <Link to=\"/\" className=\"HeaderText\">\n            <img height=\"100%\" src={LogoText} alt=\"logo\" />\n          </Link>\n          <div>\n            <nav>\n              {/* <NavLink activeClassName=\"Active\" exact to=\"/\">\n            HOME\n          </NavLink>\n          <NavLink activeClassName=\"Active\" exact to=\"/catalogue/backpacks\">\n            BACKPACKS\n          </NavLink>\n          <NavLink activeClassName=\"Active\" exact to=\"/catalogue/duffles\">\n            DUFFLES\n          </NavLink>\n          <NavLink activeClassName=\"Active\" exact to=\"/catalogue/hats\">\n            HATS\n          </NavLink>\n          <HeaderDropdown>\n            <div>ACCESSORIES</div>\n          </HeaderDropdown>\n          */}\n              <CircleButton onClick={() => setOpenSearch(!openSearch)}>\n                <i className=\"fas fa-search\"></i>\n              </CircleButton>\n              <Link to=\"/accounts\">\n                <CircleButton>\n                  <i className=\"far fa-user\"></i>\n                </CircleButton>\n              </Link>\n              {localStorage.getItem(\"userId\") ? (\n                <Link to=\"/accounts/favourites\">\n                  <CircleButton>\n                    <div className=\"Badged\">\n                      <i className=\"far fa-heart\"></i>\n                      <div className=\"BadgedCounter BadgeCounterFav\">\n                        {favouritesCount}\n                      </div>\n                    </div>\n                  </CircleButton>\n                </Link>\n              ) : null}\n              <Link to=\"/cart\">\n                <CircleButton background=\"black\">\n                  <div className=\"Badged\">\n                    <img className=\"SvgIcon\" src={Cart} alt=\"cart\" />\n                    <div className=\"BadgedCounter\">{cartCount}</div>\n                  </div>\n                </CircleButton>\n              </Link>\n              {localStorage.getItem(\"token\") ? (\n                <CircleButton onClick={logout} background=\"white\">\n                  <i className=\"fas LogoutBtn fa-sign-out-alt\"></i>\n                </CircleButton>\n              ) : null}\n            </nav>\n          </div>\n        </div>\n      </header>\n      <div\n        style={{\n          height: openSearch ? \"fit-content\" : 0,\n          padding: openSearch ? 10 : \"0 10px\",\n        }}\n        className=\"HeaderSearch\"\n      >\n        <form onSubmit={submitSearch}>\n          <input\n            ref={inputRef}\n            type=\"text\"\n            placeholder=\"search\"\n            value={searchVal}\n            onChange={(e) => setSearchVal(e.target.value)}\n          />\n        </form>\n      </div>\n      <div className=\"HeaderSpacing\"></div>\n    </>\n  );\n};\n\nexport default Header;\n","/Users/bello/Documents/qxb/src/components/UI/SideNav/SideNav.jsx",["301","302"],"import React, { useEffect, useState, usstate } from \"react\";\nimport { Link, useLocation } from \"react-router-dom\";\nimport HeaderDropdown from \"../HeaderDropdown\";\nimport \"./SideNav.css\";\nimport axios from \"axios\";\nimport { BASE_URL } from \"../../../CONFIG\";\n\nconst SideNav = ({ show, close }) => {\n  const location = useLocation();\n\n  const [Categories, setCategories] = useState([]);\n\n  const fetchCategories = () => {\n    const url = `${BASE_URL}/api/categories/`;\n    axios.get(url).then((result) => {\n      setCategories(result.data);\n    });\n  };\n\n  useEffect(() => {\n    fetchCategories();\n  }, []);\n  return (\n    <>\n      {show ? <div onClick={close} className=\"Backdrop\"></div> : null}\n      <div\n        style={{ transform: show ? \"translateX(0)\" : \"translateX(-100vw)\" }}\n        className=\"SideNav\"\n      >\n        <div className=\"CancelButton\">\n          <i onClick={close} className=\"fas fa-times fa-2x\"></i>\n        </div>\n        <div className=\"SideNav-Nav\">\n          <Link\n            onClick={close}\n            className={location.pathname === \"/\" ? \"Active\" : \"\"}\n            to=\"/\"\n          >\n            HOME\n          </Link>\n          {Categories.map((category) => (\n            <Link\n              key={category.id}\n              onClick={close}\n              className={\n                location.pathname === \"/catalogue/backpacks\" ? \"Active\" : \"\"\n              }\n              to={`/catalogue/${category.title}`}\n            >\n              {category.title}\n            </Link>\n          ))}\n          {/* <HeaderDropdown>\n            <a\n              onClick={(e) => {\n                e.preventDefault();\n              }}\n              to=\"\"\n            >\n              ACCESSORIES\n            </a>\n          </HeaderDropdown> */}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default SideNav;\n",["303","304"],"/Users/bello/Documents/qxb/src/store/actions/carts/index.js",[],["305","306"],"/Users/bello/Documents/qxb/src/components/Payment/CheckoutForm.jsx",["307"],"/Users/bello/Documents/qxb/src/components/UI/ItemInfo/ItemInfo.jsx",[],["308","309"],"/Users/bello/Documents/qxb/src/components/UI/CatalogueBanner/CatalogueBanner.jsx",[],["310","311"],"/Users/bello/Documents/qxb/src/components/UI/Alert/Alert.jsx",[],"/Users/bello/Documents/qxb/sr