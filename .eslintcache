import { Menu, Dropdown } from \"antd\";\nimport { DownOutlined } from \"@ant-design/icons\";\n\nconst menu = (\n  <Menu>\n    <Menu.Item key=\"0\">\n      <a href=\"http://www.alipay.com/\">1st menu item</a>\n    </Menu.Item>\n    <Menu.Item key=\"1\">\n      <a href=\"http://www.taobao.com/\">2nd menu item</a>\n    </Menu.Item>\n    <Menu.Divider />\n    <Menu.Item key=\"3\">3rd menu item</Menu.Item>\n  </Menu>\n);\n\nconst HeaderDropdown = ({children}) => (\n  <Dropdown overlay={menu} trigger={[\"click\"]}>\n    {children}\n  </Dropdown>\n);\n\nexport default HeaderDropdown\n","/Users/bello/Documents/qxb/src/components/Accounts/Favourites/Favourites.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/Card/Card.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/Tab/Tab.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/FeatureCategory/FeatureCategory.jsx",[],"/Users/bello/Documents/qxb/src/store/actions/accounts/index.js",[],["323","324"],"/Users/bello/Documents/qxb/src/components/UI/Loader/Loader.jsx",["325"],"import React from \"react\";\nimport \"./Loader.css\";\nimport Image from \"../../../assets/images/logo-text.png\";\n\nconst Loader = () => (\n  <div className=\"Loader\">\n    <img src={Image} />\n  </div>\n);\n\nexport default Loader;\n","/Users/bello/Documents/qxb/src/components/Login/Login.jsx",[],"/Users/bello/Documents/qxb/src/components/Accounts/Orders/Orders.jsx",["326"],"import React, { useEffect, useState } from \"react\";\nimport { Table } from \"antd\";\nimport \"./Orders.css\";\nimport { useLocation } from \"react-router-dom\";\nimport { BASE_URL } from \"../../../CONFIG\";\nimport Pagination from \"../../UI/Pagination/Pagination\";\nimport axios from \"axios\";\nimport Alert from \"../../UI/Alert/Alert\";\n\nconst Orders = () => {\n  const [loading, setLoading] = useState(false);\n  const [error, setError] = useState(false);\n  const [orders, setOrders] = useState();\n  const [next, setNext] = useState();\n  const [previous, setPrevious] = useState();\n\n  const location = useLocation();\n\n  const columns = [\n    {\n      title: \"\",\n      dataIndex: \"img\",\n      render: (src) => {\n        return (\n          <img className=\"OrderImage\" width=\"50\" height=\"50\" alt=\"\" src={src} />\n        );\n      },\n    },\n    {\n      title: \"Name of product\",\n      dataIndex: \"name\",\n    },\n    {\n      title: \"Amount\",\n      dataIndex: \"amount\",\n      render: (text) => <b>{`$${text}`}</b>,\n    },\n    {\n      title: \"Quantity\",\n      dataIndex: \"quantity\",\n    },\n    {\n      title: \"Sub-Total Amount\",\n      dataIndex: \"subTotal\",\n      render: (text) => <b>{`$${text}`}</b>,\n    },\n  ].filter((item) => {\n    if (window.screen.width >= 500) {\n      return item;\n    } else {\n      return item.title !== \"\";\n    }\n  });\n\n  const updatedData = (data = []) =>\n    data?.map((item) => ({\n      key: item.id,\n      img: BASE_URL + item.product_obj.image,\n      name: item.product_obj.title,\n      amount: item.product_obj.price,\n      quantity: item.quantity,\n      subTotal: item.sub_total,\n    }));\n\n  const fetchOrder = (newUrl = null) => {\n    const id = location.pathname.split(\"/\")[3];\n    const token = localStorage.getItem(\"token\");\n    let url = `${BASE_URL}/api/order-items/?order=${id}`;\n    if (newUrl) {\n      url = newUrl;\n    }\n    const headers = {\n      Authorization: `JWT ${token}`,\n    };\n    setLoading(true);\n    setError(false);\n    axios\n      .get(url, { headers: headers })\n      .then((result) => {\n        setLoading(false);\n        setNext(result.data.next);\n        setPrevious(result.data.previous);\n        setOrders(result.data.results);\n      })\n      .catch(() => {\n        setLoading(false);\n        setError(true);\n        setTimeout(() => {\n          setError(false);\n        }, 3000);\n      });\n  };\n\n  useEffect(() => {\n    fetchOrder();\n  }, []);\n\n  return (\n    <div className=\"Orders\">\n      <Alert show={error} message=\"something went wrong\" error={true} />\n      <Table\n        loading={loading}\n        columns={columns}\n        dataSource={updatedData(orders)}\n        pagination={false}\n      />\n      <Pagination\n        onClickNext={next ? () => fetchOrder(next) : null}\n        onClickPrevious={previous ? () => fetchOrder(previous) : null}\n      />\n    </div>\n  );\n};\n\nexport default Orders;\n","/Users/bello/Documents/qxb/src/components/Accounts/Orders/OrdersDetail.jsx",["327"],"import React, { useEffect, useState } from \"react\";\nimport { Col, Divider, Row, Table } from \"antd\";\nimport { useLocation } from \"react-router\";\nimport axios from \"axios\";\nimport { BASE_URL } from \"../../../CONFIG\";\nimport Loader from \"../../UI/Loader/Loader\";\nimport ErrorComponent from \"../../UI/ErrorComponent/ErrorComponent\";\n\nconst Item = ({ key_, value }) => (\n  <Row style={{ margin: \"10px 0\" }} gutter={20}>\n    <Col style={{ fontWeight: \"bold\" }} sm={6}>\n      {key_}\n    </Col>\n    <Col sm={18}>{value}</Col>\n  </Row>\n);\n\nconst OrdersDetail = () => {\n  const [total, setTotal] = useState(0);\n  const [loading, setLoading] = useState(false);\n  const [error, setError] = useState(false);\n  const [data, setData] = useState(false);\n  const [contact, setContact] = useState();\n\n  const columns = [\n    {\n      title: \"Name\",\n      dataIndex: \"name\",\n      render: (_, dataRow) => (\n        <div className=\"CartName\">\n          <img width=\"50\" height=\"50\" alt=\"\" src={dataRow.img} />\n          <p>{dataRow.name}</p>\n        </div>\n      ),\n    },\n    {\n      title: \"Quantity\",\n      dataIndex: \"quantity\",\n    },\n    {\n      title: \"Sum\",\n      dataIndex: \"amount\",\n      render: (text) => <b>{`$${text}`}</b>,\n    },\n  ];\n  const location = useLocation();\n\n  const fetchOrder = () => {\n    const id = location.pathname.split(\"/\")[2];\n    setLoading(true);\n    setError(false);\n    axios\n      .get(`${BASE_URL}/api/order-items/?order=${id}`)\n      .then((res) => {\n        setContact(res.data.order_details);\n        const data = res.data.order_items?.map((item) => ({\n          key: item.id,\n          img: BASE_URL + item.product_obj.image,\n          name: item.product_obj.title,\n          amount: item.product_obj.price,\n          quantity: item.quantity,\n          subTotal: item.sub_total,\n        }));\n        setData(data);\n        setTotal(\n          res.data.order_items[0] ? res.data.order_items[0].total_amount : 0\n        );\n        setLoading(false);\n        setError(false);\n      })\n      .catch((err) => {\n        console.log(err);\n        setLoading(false);\n        setError(true);\n      });\n  };\n\n  useEffect(() => {\n    fetchOrder();\n  }, []);\n\n  return (\n    <>\n      {loading ? <Loader /> : null}\n      {error ? <ErrorComponent onReload={fetchOrder} /> : null}\n      {data ? (\n        <div\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            width: \"90%\",\n            margin: \"0 auto\",\n          }}\n        >\n          <h1>Order Summary</h1>\n          <Divider />\n          <div className=\"OrderContact\">\n            <h2>Order Contact Info</h2>\n            <Item key_={\"Name\"} value={contact.name} />\n            <Item key_={\"Email\"} value={contact.email} />\n            <Item key_={\"Phonenumber\"} value={contact.phone_number} />\n            <Item key_={\"Order Date\"} value={contact.date} />\n            <Item key_={\"Country\"} value={contact.country} />\n            <Item key_={\"City\"} value={contact.city} />\n            <Item key_={\"Shipping Adress\"} value={contact.shipping_address} />\n            <Item key_={\"Postal Code\"} value={contact.postal_code} />\n          </div>\n          <Divider />\n          <h2>Order Items</h2>\n          <Table\n            style={{ width: \"100%\", marginTop: 40 }}\n            columns={columns}