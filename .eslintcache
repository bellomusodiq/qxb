esult.data.results);\n        }\n      })\n      .catch(() => {\n        setLoading(false);\n        setError(true);\n        setMessage(\"Oops, Something went wrong, Try again!\");\n        setShowSnackbar(true);\n        setTimeout(() => {\n          setShowSnackbar(false);\n        }, 3000);\n      });\n  };\n\n  const fetchRecommendedProducts = () => {\n    const url = `${BASE_URL}/api/products/?recommended=true`;\n    axios.get(url)\n      .then(result => {\n        setRecommendedProducts(result.data.results);\n      })\n  }\n\n  const getUrl = () => {\n    let url = `${BASE_URL}/api/products/`;\n    const category = parseCategory(location.pathname);\n    if (category) {\n      url += `?category=${category}`;\n    }\n    return url;\n  }\n  useEffect(() => {\n    const url = getUrl();\n    setInitialLoad(true);\n    fetchProducts(url, true);\n  }, [location]);\n\n  useEffect(() => {\n    fetchRecommendedProducts();\n  }, [])\n\n  let bodyContent;\n  if (loading && initialLoad) {\n    bodyContent = <Loader />;\n  } else if (error && !products) {\n    bodyContent = <ErrorComponent onReload={() => fetchProducts(getUrl(), true)} />;\n  } else if (products) {\n    bodyContent = (\n      <>\n        <Alert error={error} show={showSnackBar} message={message} />\n        <div className=\"BannerArea\">\n          <CatalogueBanner show />\n        </div>\n        <div className=\"Catalogue\">\n          <div className=\"ProductItems\">\n            {products?.map((product) => (\n              <ProductItem\n                key={product.id}\n                id={product.id}\n                price={product.price}\n                title={product.title}\n                image={product.image}\n                reviewsCount={product.reviews_count}\n                rating={product.rating}\n              />\n            ))}\n          </div>\n          {nextPage ? (\n            <div className=\"LoadMore\">\n              <DefaultButton\n                loading={products && loading}\n                onClick={() => fetchProducts(nextPage)}\n              >\n                LOAD MORE\n              </DefaultButton>\n            </div>\n          ) : null}\n          <Recommendation data={recommendedProducts} title=\"Recommended\" />\n        </div>\n      </>\n    );\n  } else {\n    bodyContent = null;\n  }\n\n  return bodyContent;\n};\n\nexport default Catalogue;\n","/Users/bello/Documents/qxb/src/components/Checkout/Checkout.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/Buttons/Buttons.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/ErrorComponent/ErrorComponent.jsx",[],"/Users/bello/Documents/qxb/src/components/CategorySection/Filter/Filter.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/CustomerReview/CustomerReview.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/Pagination/Pagination.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/ProductItems/ProductItem/ProductItem.jsx",[],"/Users/bello/Documents/qxb/src/store/actions/reviews/index.js",[],"/Users/bello/Documents/qxb/src/components/Accounts/Reviews/Reviews.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/MarkerSelect/MarkerSelect.jsx",[],"/Users/bello/Documents/qxb/src/store/reducer/account/index.js",[],"/Users/bello/Documents/qxb/src/reportWebVitals.js",[],"/Users/bello/Documents/qxb/src/components/UI/Recommendation/Recommendation.jsx",[],"/Users/bello/Documents/qxb/src/components/StoreFront/BlogList/BlogList.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/LoadingComponent/LoadingComponent.jsx",[],"/Users/bello/Documents/qxb/src/components/UI/Summary/Summary.jsx",[],"/Users/bello/Documents/qxb/src/components/Checkout/Contact/Contact.jsx",[],"/Users/bello/Documents/qxb/src/store/actions/orders/index.js",[],"/Users/bello/Documents/qxb/src/store/reducer/orders/index.js",[],"/Users/bello/Documents/qxb/src/store/reducer/carts/index.js",[],"/Users/bello/Documents/qxb/src/components/Payment/Payment.jsx",["283"],"/Users/bello/Documents/qxb/src/store/re